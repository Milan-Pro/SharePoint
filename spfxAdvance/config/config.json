{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "prop-pane-demo-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/propPaneDemo/PropPaneDemoWebPart.js",
          "manifest": "./src/webparts/propPaneDemo/PropPaneDemoWebPart.manifest.json"
        }
      ]
    },
    "prop-pane-react-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/propPaneReact/PropPaneReactWebPart.js",
          "manifest": "./src/webparts/propPaneReact/PropPaneReactWebPart.manifest.json"
        }
      ]
    },
    "spfx-lib-library": {
      "components": [
        {
          "entrypoint": "./lib/libraries/spfxLib/SpfxLibLibrary.js",
          "manifest": "./src/libraries/spfxLib/SpfxLibLibrary.manifest.json"
        }
      ]
    },
    "producer-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/producer/ProducerWebPart.js",
          "manifest": "./src/webparts/producer/ProducerWebPart.manifest.json"
        }
      ]
    },
    "consumer-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/consumer/ConsumerWebPart.js",
          "manifest": "./src/webparts/consumer/ConsumerWebPart.manifest.json"
        }
      ]
    },
    "graph-demo-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/graphDemo/GraphDemoWebPart.js",
          "manifest": "./src/webparts/graphDemo/GraphDemoWebPart.manifest.json"
        }
      ]
    },
    "azure-func-demo-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/azureFuncDemo/AzureFuncDemoWebPart.js",
          "manifest": "./src/webparts/azureFuncDemo/AzureFuncDemoWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "PropPaneDemoWebPartStrings": "lib/webparts/propPaneDemo/loc/{locale}.js",
    "PropPaneReactWebPartStrings": "lib/webparts/propPaneReact/loc/{locale}.js",
    "SpfxLibLibraryStrings": "lib/libraries/spfxLib/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "ProducerWebPartStrings": "lib/webparts/producer/loc/{locale}.js",
    "ConsumerWebPartStrings": "lib/webparts/consumer/loc/{locale}.js",
    "GraphDemoWebPartStrings": "lib/webparts/graphDemo/loc/{locale}.js",
    "AzureFuncDemoWebPartStrings": "lib/webparts/azureFuncDemo/loc/{locale}.js"
  }
}
